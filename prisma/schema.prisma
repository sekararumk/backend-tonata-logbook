generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// prisma/schema.prisma
model Logbook {
  id          Int      @id @default(autoincrement())
  // ... kolom lain seperti judul, keterangan, dll.
  link_file   String?  // <-- PASTIKAN BARIS INI ADA. Tanda tanya (?) berarti opsional.
}